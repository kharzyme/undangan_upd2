{
  "version": 2,
  "builds": [
    { "src": "public/index.php", "use": "vercel-php@0.7.2" }
  ],
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "routes": [
    { "src": "/build/(.*)", "dest": "public/build/$1" },
    { "src": "/codes/script.js", "dest": "public/codes/script.js" },
    { "src": "/images/Penjaga_Hati.mp3", "dest": "public/images/Penjaga_Hati.mp3" },
    { "src": "/fontawesome/css/(.*)", "dest": "public/fontawesome/css/$1" },
    { "src": "/fontawesome/webfonts/(.*)", "dest": "public/fontawesome/webfonts/$1" },
    { "src": "/(.*)", "dest": "public/index.php" }
  ]
}

