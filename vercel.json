{
  "version": 3,
  "builds": [
    {
      "src": "public/index.php",
      "use": "@vercel/php",
      "config": { "runtime": "php-8.2" }
    },
    {
      "src": "resources/js/app.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public/build"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "public/index.php" }
  ]
}
